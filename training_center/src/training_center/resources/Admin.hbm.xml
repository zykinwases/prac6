<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="database.Admin" table="admin_acc">
		<id column="admin_id" name="id" type="java.lang.Long">
			<generator class="increment"/>
		</id>
		<property column="login" name="login" type="java.lang.String"/>
		<property column="pswd_hash" name="pswd_hash" type="java.lang.String"/>
		<property column="last_name" name="last_name" type="java.lang.String"/>
		<property column="first_name" name="first_name" type="java.lang.String"/>
	</class>
</hibernate-mapping>