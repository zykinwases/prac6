<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="database.Company" table="company">
		<id column="company_id" name="id" type="java.lang.Long">
			<generator class="increment"></generator>
		</id>
		<property column="name" name="name" type="java.lang.Long"></property>
		<property column="address" name="address" type="java.lang.String"/>
		<set name="staff" lazy="false">
			<key column="company_id"/>
			<one-to-many class="database.Professor"/>
		</set>
	</class>
</hibernate-mapping>