<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="database.Lesson" table="lesson">
		<id column="lesson_id" name="id" type="java.lang.Long">
			<generator class="increment"/>
		</id>
		<many-to-one name="course_id" column="course_id" class="databse.Course"/>
		<property column="time" name="time" type="java.sql.Timestamp"/>
	</class>
</hibernate-mapping>